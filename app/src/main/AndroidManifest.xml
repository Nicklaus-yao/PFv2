<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nykidxxx.pfv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.InputPage"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.HistoryPage"
            android:screenOrientation="portrait"></activity>

        <provider
            android:name=".model.TransactionProvider"
            android:authorities="com.nykidxxx.pfv2.model.TransactionProvider"
            android:multiprocess="true" />

        <activity android:name=".ui.EditPage"></activity>
    </application>

</manifest>